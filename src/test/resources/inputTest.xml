<?xml version="1.0" encoding="UTF-8"?>
<simpleCalculator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="../../main/resources/SimpleCalculator.xsd">
