        <expression>
            <operation OperationType="DIV">
                <operation OperationType="MUL">
                    <operation OperationType="DIV">
                        <arg>13</arg>
                        <arg>43</arg>
                    </operation>
                    <operation OperationType="DIV">
                        <arg>3995</arg>
                        <arg>2</arg>
                    </operation>
                </operation>
                <operation OperationType="SUM">
